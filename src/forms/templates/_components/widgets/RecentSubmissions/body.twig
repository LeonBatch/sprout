{% if submissions|length %}
    <table class="data fullwidth collapsible">
        <tbody>
        {% for submission in submissions %}
            <tr>
                <td data-title="{{ 'Submission'|t('sprout-module-forms') }}">
                    {% if currentUser.can('accessPlugin-SproutForms') -%}
                        <a href="{{ submission.getCpEditUrl() }}"
                            title="{{ 'Edit submission'|t('sprout-module-forms') }}">{{ submission.title }}</a>
                    {% else -%}
                        {{ submission.title }}
                    {% endif %}
                    {% if widget.formId == 0 -%}
                        <br>
                        {{ submission.formName }} â€“ <span
                        class="light">{{ submission.dateCreated|date('n/j/Y') }}</span>
                    {% endif %}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% else %}
    <p>{{ 'No recent submissions available.'|t('sprout-module-forms') }}</p>
{% endif %}
